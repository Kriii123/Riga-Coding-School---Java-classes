
public class Board {
	
	String[][] gameBoard;
	
	public Board() {
		gameBoard = new String[3][3];
	}
	
	public void printGameBoard() {
		
		gameBoard = new String[][] {{"X","","O"},{"","O",""},{"O","X","X"}};
		
		for(int i=0;i < gameBoard.length;i++) {
			for(int j=0;j<gameBoard.length;j++) {
				System.out.println(gameBoard[i][j]);
				
			}
		}
		
	}
	

}
