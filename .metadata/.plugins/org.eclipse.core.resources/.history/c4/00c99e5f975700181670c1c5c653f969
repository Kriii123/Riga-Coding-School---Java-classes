
public class Main {

	public static void main(String[] args) {
		Board board = new Board();
		board.printGameBoard();
		System.out.println();
		board.makeMove("X", 0, 1);
		board.printGameBoard();
		
		System.out.println("X has won: " + board.hasPlayerWon("X"));
		System.out.println("X has won: " + board.hasPlayerWon("O"));
		HumanPlayer p1 = new HumanPlayer(board);
		
		int[] coordinates = p1.makeMove();
		board.makeMove("X", coordinates[0], coordinates[1]);
		board.printGameBoard();
		

	}

}
